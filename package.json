{
  "name": "webrtcbasep2p",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "engines": {
    "node": "14.16.1"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.ejs src/**/*.html src/**/*.css dist/",
    "start": "npm run clean && npm run tsc && npm run copy-files && node dist/app.js",
    "test": "nyc ./node_modules/.bin/_mocha 'tests/**/*.test.ts' ",
    "postinstall": "tsc && mv -i -v src/public dist/ && mv -i -v views dist/ && rm -v -rf src && rm -v -rf tests",
    "dev": "nodemon -r dotenv/config src/app.ts",
    "tsc": "tsc",
    "watch-node": "nodemon dist/app.js",
    "watch-ts": "tsc -w",
    "deploy": "git add . && git commit -m Heroku && git push heroku master"
  },
  "nodemonConfig": {
    "ignore": [
      "tests/*"
    ],
    "delay": 100
  },
  "author": "Gustavo Navarro",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.1",
    "helmet": "^4.5.0",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.8",
    "mongoose-unique-validator": "^2.0.3",
    "passport": "^0.4.1",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "socket.io": "^4.0.1",
    "socket.io-client": "^4.0.1",
    "sweetalert2": "^10.16.6",
    "toastr": "^2.1.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.1.2",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-jwt": "^6.0.1",
    "@types/express-session": "^1.17.3",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose": "^5.10.5",
    "@types/mongoose-unique-validator": "^1.0.4",
    "@types/node": "^14.14.41",
    "@types/passport": "^1.0.6",
    "@types/passport-anonymous": "^1.0.3",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/passport-local-mongoose": "^4.0.13",
    "@types/selenium-standalone": "^6.15.2",
    "@types/selenium-webdriver": "^4.0.12",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.36",
    "@types/uuid": "^8.3.0",
    "chai": "^4.3.4",
    "copyfiles": "^2.4.1",
    "dotenv": "^9.0.2",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
